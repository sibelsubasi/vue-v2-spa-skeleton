{
  "name": "vue-spa-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host localhost",
    "test-build": "cp ./.env.production ./.env.temp && cp ./.env.test ./.env.production && vue-cli-service build && cp ./.env.temp ./.env.production && rm ./.env.temp",
    "dev-build": "cp ./.env.production ./.env.temp && cp ./.env.development ./.env.production && vue-cli-service build && cp ./.env.temp ./.env.development && rm ./.env.temp",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^4.5.95",
    "apexcharts": "^3.26.3",
    "axios": "^0.21.2",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.3.2",
    "crypto-js": "^4.1.1",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "null-loader": "^4.0.0",
    "sortablejs": "^1.14.0",
    "v-mask": "^2.2.3",
    "vue": "^2.6.10",
    "vue-advanced-cropper": "^0.16.5",
    "vue-apexcharts": "^1.6.1",
    "vue-i18n": "^8.15.1",
    "vue-matomo": "^4.1.0",
    "vue-router": "^3.1.3",
    "vue-worker": "^1.2.1",
    "vuedl": "^0.3.10",
    "vuetify": "^2.1.0",
    "vuetify-datetime-picker": "^2.1.1",
    "vuetify-dialog": "^2.0.0-rc.0",
    "vuex": "^3.0.1",
    "xlsx": "^0.16.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "font-awesome": "4.7.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "semi": "off",
      "indent": "off",
      "quotes": "off",
      "no-unused-labels": "off",
      "comma-dangle": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-unreachable": "off",
      "no-constant-condition": "off",
      "space-before-function-paren": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 major version",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 9",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
